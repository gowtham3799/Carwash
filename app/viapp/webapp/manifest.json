{
  "_version": "1.37.0",
  "sap.app": {
    "id": "viapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.1",
      "toolsId": "e9e2afd3-a73d-4f47-bd69-9c6a787a9e1f"
    },
    "dataSources": {
      "CarwashService": {
        "uri": "/odata/v2/CarwashService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "viapp-display": {
          "semanticObject": "viapp",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        },
        "vehicleapp-manage": {
          "semanticObject": "vehicleapp",
          "action": "manage",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.96.36",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "viapp.i18n.i18n"
        }
      },
      "CarwashService": {
        "dataSource": "CarwashService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ],
      "js": [
				{
					"uri": "Js/grid.js"
				},
				{
					"uri": "Js/version.js"
				},
				{
					"uri": "Js/detector.js"
				},
				{
					"uri": "Js/formatinf.js"
				},
				{
					"uri": "Js/errorlevel.js"
				},
				{
					"uri": "Js/bitmat.js"
				},
				{
					"uri": "Js/datablock.js"
				},
				{
					"uri": "Js/bmparser.js"
				},
				{
					"uri": "Js/datamask.js"
				},
				{
					"uri": "Js/rsdecoder.js"
				},
				{
					"uri": "Js/gf256poly.js"
				},
				{
					"uri": "Js/gf256.js"
				},
				{
					"uri": "Js/decoder.js"
				},
				{
					"uri": "Js/qrcode.js"
				},
				{
					"uri": "Js/alignpat.js"
				},
				{
					"uri": "Js/databr.js"
				},
				{
					"uri":"Js/crypto.js"
				}
			]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "viapp.view",
        "controlAggregation": "pages",
        "controlId": "pageContainer",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "MainMenu",
          "pattern": "MainMenu",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "MainMenu"
          ]
        },
        {
          "pattern": "Home",
          "name": "Home",
          "target": [
            "Home"
          ]
        },
        {
          "name": "Services",
          "pattern": "Services",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "Services"
          ]
        },
        {
          "name": "CreateCustomer",
          "pattern": "CreateCustomer",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "CreateCustomer"
          ]
        },
        {
          "name": "Report",
          "pattern": "Report",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "Report"
          ]
        }
      ],
      "targets": {
        "Home": {
          "viewType": "XML",
          "viewName": "Home",
          "clearControlAggregation": false
        },
        "MainMenu": {
          "viewType": "XML",
          "transition": "slide",
          "clearAggregation": true,
          "viewName": "MainMenu"
        },
        "Services": {
          "viewType": "XML",
          "transition": "slide",
          "clearAggregation": true,
          "viewName": "Services"
        },
        "CreateCustomer": {
          "viewType": "XML",
          "transition": "slide",
          "clearAggregation": true,
          "viewName": "CreateCustomer"
        },
        "Report": {
          "viewType": "XML",
          "transition": "slide",
          "clearAggregation": true,
          "viewName": "Report"
        }
      }
    },
    "rootView": {
      "viewName": "viapp.view.MainMenu",
      "type": "XML",
      "async": true,
      "id": "splitapp"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "vehicleapp"
  }
}